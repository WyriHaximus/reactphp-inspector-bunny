{
    "name": "wyrihaximus/react-inspector-bunny",
    "license": "MIT",
    "description": "Bunny bindings",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "wyrihaximus/react-inspector": "dev-master"
    },
    "require-dev": {
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "files": ["src/include.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\Inspector\\Bunny\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
